{
	"name": "boilerplate-sdk",
	"version": "3.0.2",
	"description": "boilerplate-sdk",
	"author": "Conexa",
	"engines": {
		"node": ">=22.0.0"
	},
	"scripts": {
		"install:all": "yarn install --frozen-lockfile",
		"install:boiler": "yarn install --check-files",
		"build": "tsc && tsc-alias",
		"build:watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
		"typecheck": "yarn tsc --project ./tsconfig-typecheck.json",
		"test": "yarn jest --detectOpenHandles --passWithNoTests",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"prettier": "prettier --check **/*.ts",
		"prettier:fix": "prettier --write **/*.ts",
		"release": "yarn build && yarn standard-version",
		"release:first": "yarn build && yarn standard-version --first-release",
		"release:minor": "yarn build && yarn standard-version --release-as minor",
		"release:patch": "yarn build && yarn standard-version --release-as patch",
		"release:major": "yarn build && yarn standard-version --release-as major",
		"publish:registry": "yarn build && git push --tags && npm publish --registry https://npm-registry.conexa.ai"
	},
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"license": "MIT",
	"files": [
		"dist/"
	],
	"devDependencies": {
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.30.1",
		"@jest/globals": "^30.0.4",
		"@types/jest": "^30.0.0",
		"@types/node": "^24.0.13",
		"@typescript-eslint/eslint-plugin": "^8.36.0",
		"@typescript-eslint/parser": "^8.36.0",
		"eslint": "^9.30.1",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.5.1",
		"jest": "^30.0.4",
		"prettier": "^3.6.2",
		"standard-version": "^9.5.0",
		"ts-alias": "^0.0.7",
		"ts-jest": "^29.4.0",
		"tsc-alias": "^1.8.16",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"axios": "1.10.0",
		"pino": "^9.7.0",
		"pino-pretty": "^13.0.0"
	}
}
